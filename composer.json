{
    "name": "johnwhitley/seperate-eloquent-concerns",
    "description": "Laravel eloquent has a lot in the model class, this package helps with separating the concerns away from just the model",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "John Whitley",
            "email": "j.n.whitley@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "John\\Eloquent\\": "John/Eloquent/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "John\\Eloquent\\Test\\": "test-src/"
        }
    },
    "require": {
        "laravel/framework": "^5.3"
    },
    "bin": [
    ],
    "scripts": {
        "test": [
            "@phpunit",
            "@phpmd",
            "@phpcs"
        ],
        "phpunit": "phpunit",
        "phpmd": "phpmd John/ text cleancode,codesize,controversial,design,naming,unusedcode",
        "phpcs": "phpcs -s --standard=./CodeSnifferRuleset.xml John",
        "phpcbf": "phpcbf -s --standard=PSR2 John"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "orchestra/testbench": "^3.3",
        "squizlabs/php_codesniffer": "2.*",
        "phpmd/phpmd": "^2.5"
    },
    "support": {
        "issues": "https://github.com/john-whitley/seperate-eloquent-concerns/issues"
    }
}
